#include<stdio.h>
int main(){
    int arr[10]={2,5,8,10,3,7,6,9,4,1};
    int even_count=0,odd_count=0;
    for(int i=0;i<10;i++){
        if(arr[i]%2==0){
            even_count++;
        }else{
            odd_count++;
        }
    }
    printf("Array values:");
    for(int i=0;i<10;i++){
        printf("%d",arr[i]);
    }
    printf("\nCount of even numbers:%d\n",even_count);
    printf("Count of odd numbers:%d\n",odd_count);
    return 0;
}
